@using HCMS.Dynamics.Tools
@{
    ViewBag.title = " وارد کردن بخش های جدید";
}




<form method="post" class="box-bordered danger">
    <h1>
        <i class="fa fa-download"></i>
       @ViewBag.title
    </h1>

@Html.ValidationSummary()
    <textarea name="input" placeholder="Copy & Past your export file contents ..." style="display:block;width:100%;height:400px;direction:ltr;font-family:Tahoma;font-size:12px;"></textarea>

    <div class="buttons">
        <input type="submit" class="btn btn-default" value="ارسال فایل" />
    </div>
</form>
